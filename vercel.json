{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/tours",
            "dest": "/server.js"
        },
        {
            "src": "/api/v1/users",
            "dest": "/server.js"
        }
    ]
}
